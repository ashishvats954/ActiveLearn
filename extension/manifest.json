{
  "manifest_version": 3,
  "name": "ActiveLearn",
  "version": "0.1",
  "description": "Generates summaries and quizzes from video lectures.",

  

  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  }
}